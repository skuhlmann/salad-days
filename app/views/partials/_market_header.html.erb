<div id="sub-line" class="column-group">
  <div class="all-75">
    <h2>Search Results</h2>
  </div>
  <div class="all-25 zip">
    <% if successful_search? %>
      <h5><%= markets.count %> results for <%= params[:zip] %></h5>
    <% else %>
      <div id="page-form" class="ink-form">
        <div class="control-group">
          <div class="control">
            <%= render partial: "partials/search_form" %>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>


